{
    "components": [
        {
            "type": "Label",
            "props": {
                "label": "Components"
            }
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "DateTimePicker",
                    "props": {
                        "tooltip": "DateTimePicker tooltip",
                        "label": "DateTimePicker",
                        "value": "2020-09-18"
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Checkbox",
                    "props": {
                        "tooltip": "Checkbox tooltip",
                        "label": "Checkbox",
                        "value": true
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Icon",
                    "props": {
                        "label": "Icon",
                        "type": "edit",
                        "size": 20,
                        "tooltip": {
                            "type": "Typography",
                            "props": {
                                "type": "h1",
                                "value": "Icon tooltip"
                            }
                        }
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Input",
                    "props": {
                        "tooltip": [
                            {
                                "type": "Typography",
                                "props": {
                                    "type": "h3",
                                    "value": "Input"
                                }
                            },
                            {
                                "type": "Typography",
                                "props": {
                                    "type": "h4",
                                    "value": "tooltip"
                                }
                            }
                        ],
                        "label": "Input",
                        "value": "Input",
                        "type": "wysiwyg"
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Label",
                    "props": {
                        "label": "Label",
                        "required": true
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "PeriodSelector",
                    "props": {
                        "label": "Period Selector",
                        "periodType": "custom",
                        "tooltip": "Period Selector tooltip"
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "RadioButton",
                    "props": {
                        "label": "Radio button",
                        "value": true,
                        "tooltip": "Radio button tooltip"
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Range",
                    "props": {
                        "value": true,
                        "tooltip": "Range tooltip"
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Modal",
                    "props": {
                        "children": "modal",
                        "isOpen": {
                            "$type": "$data",
                            "path": "$page.isModalOpen[\"modal\"]"
                        },
                        "close": {
                            "$type": "$action",
                            "path": "$page.closeModal",
                            "args": ["modal"]
                        }
                    }
                },
                {
                    "type": "Button",
                    "props": {
                        "tooltip": "Button tooltip",
                        "label": "open modal",
                        "onClick": {
                            "$type": "$action",
                            "path": "$page.openModal",
                            "args": ["modal"]
                        }
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Popup",
                    "props": {
                        "children": "popup",
                        "bottom": 80,
                        "isOpen": {
                            "$type": "$data",
                            "path": "$page.isPopupOpen[\"modal\"]"
                        }
                    }
                },
                {
                    "type": "Button",
                    "props": {
                        "label": "open popup",
                        "onClick": {
                            "$type": "$action",
                            "path": "$page.openPopup",
                            "args": ["modal"]
                        }
                    }
                }
            ]
        },
        {
            "type": "Spacer",
            "props": {
                "size": 16
            }
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h1",
                        "value": "Title"
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h2",
                        "value": "Title",
                        "bold": true
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h3",
                        "value": "Title"
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h4",
                        "value": "Title",
                        "style": {
                            "color": "#F00"
                        }
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h5",
                        "value": "Title"
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "h6",
                        "value": "Title",
                        "bold": true
                    }
                }
            ]
        },
        {
            "type": "Cell",
            "components": [
                {
                    "type": "Typography",
                    "props": {
                        "type": "label",
                        "bold": true
                    },
                    "components": [
                        {
                            "type": "Typography",
                            "props": {
                                "type": "h2",
                                "value": "Title"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
