            "nodes": [
                {
                    "type": "Block",
                    "props": {
                        "transparent": true
                    },
                    "nodes": [
                        {
                            "type": "Menu",
                            "props": {
                                "style": {
                                    "color": "var(--trood-blue)"
                                },
                                "itemActiveStyle": {
                                    "color": "var(--trood-black)"
                                },
                                "type": "horizontal",
                                "basePath": "/clients",
                                "items": [
                                    {
                                        "path": "/list",
                                        "title": "List"
                                    },
                                    {
                                        "path": "/table",
                                        "title": "Table"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        },
        {
            "type": "Route",
            "props": {
                "path": "/clients/list",
                "title": "Clients List"
            },
            "nodes": [
                {
                    "type": "LoadingIndicator"
                }
            ],
            "chunk": "example/clients.json"
        },
        {
            "type": "Route",
            "props": {
                "path": "/clients/table",
                "title": "Clients Table"
            },
            "nodes": [
