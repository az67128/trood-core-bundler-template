{
    "nodes": [
        {
            "type": "Route",
            "props": {
                "path": "/",
                "title": "Root",
                "exact": true
            },
            "nodes": [
                {
                    "type": "div",
                    "props": {
                        "children": "Main"
                    }
                }
            ]
        },
        {
            "type": "Route",
            "props": {
                "path": "/clients",
                "title": "Clients"
