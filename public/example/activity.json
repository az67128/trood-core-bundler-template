{
    "components": [
        {
            "name": "Context",
            "props": {
                "context": {
                    "$type": "$data",
                    "path": "$store.apis.custodian.activity.getByPk[{\"$type\":\"$expression\",\"expression\":\"+data('$route.params.id')\"}]"
                }
            },
            "components": [
                {
                    "name": "Cell",
                    "components": [
                        {
                            "name": "Cell",
                            "props": {
                                "children": {
                                    "$type": "$data",
                                    "path": "$route.params.id"
                                }
                            }
                        },
                        {
                            "name": "Cell",
                            "props": {
                                "children": {
                                    "$type": "$data",
                                    "path": "$context.name"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
