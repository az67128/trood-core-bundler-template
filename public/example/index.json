{
    "components": [
        {
            "name": "Container",
            "props": {
                "fluid": true
            },
            "components": [
                {
                    "name": "Row",
                    "props": {
                        "style": {
                            "height": "100vh",
                            "backgroundColor": "var(--trood-background)"
                        },
                        "noGutters": true
                    },
                    "components": [
                        {
                            "name": "Cell",
                            "props": {
                                "xsAuto": true,
                                "style": {
                                    "zIndex": 1
                                }
                            },
                            "components": [
                                {
                                    "name": "div",
                                    "props": {
                                        "style": {
                                            "height": "100%",
                                            "boxShadow": "0 0 15px 0 var(--trood-shadow)",
                                            "backgroundColor": "var(--trood-white)"
                                        }
                                    },
                                    "components": [
                                        {
                                            "name": "Cell",
                                            "props": {
                                                "style": {
                                                    "padding": "8px"
                                                }
                                            },
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/"
                                                    },
                                                    "components": [
                                                        {
                                                            "name": "img",
                                                            "props": {
                                                                "src": "/images/logo.svg"
                                                            }
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/clients",
                                                        "children": {
                                                            "$type": "$expression",
                                                            "expression": "data(\"$route.location.pathname\") == \"/clients\" ? \"Clients*\" : \"Clients\""
                                                        }
                                                    }
                                                }
                                            ]
                                        },
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/clients_table",
                                                        "children": {
                                                            "$type": "$expression",
                                                            "expression": "data(\"$route.location.pathname\") == \"/clients_table\" ? \"Clients Table*\" : \"Clients Table\""
                                                        }
                                                    }
                                                }
                                            ]
                                        },
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/matters",
                                                        "children": {
                                                            "$type": "$expression",
                                                            "expression": "data(\"$route.location.pathname\") == \"/matters\" ? \"Matters*\" : \"Matters\""
                                                        }
                                                    }
                                                }
                                            ]
                                        },
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/remote",
                                                        "children": {
                                                            "$type": "$expression",
                                                            "expression": "data(\"$route.location.pathname\") == \"/remote\" ? \"Remote*\" : \"Remote\""
                                                        }
                                                    }
                                                }
                                            ]
                                        },
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Link",
                                                    "props": {
                                                        "to": "/components",
                                                        "children": {
                                                            "$type": "$expression",
                                                            "expression": "data(\"$route.location.pathname\") == \"/components\" ? \"Components*\" : \"Components\""
                                                        }
                                                    }
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "name": "Cell",
                            "props": {
                                "style": {
                                    "overflowX": "hidden",
                                    "overflowY": "auto",
                                    "height": "100%"
                                },
                                "id": "MAIN_SCROLL_CONTAINER"
                            },
                            "components": [
                                {
                                    "name": "Row",
                                    "props": {
                                        "style": {
                                            "backgroundColor": "var(--trood-white)"
                                        }
                                    },
                                    "components": [
                                        {
                                            "name": "Cell",
                                            "props": {
                                                "xsAuto": true,
                                                "children": "back"
                                            }
                                        },
                                        {
                                            "name": "Cell",
                                            "props": {
                                                "children": "tabs"
                                            }
                                        },
                                        {
                                            "name": "Cell",
                                            "props": {
                                                "xsAuto": true,
                                                "children": "avatar"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "name": "Row",
                                    "components": [
                                        {
                                            "name": "Cell",
                                            "components": [
                                                {
                                                    "name": "Switch",
                                                    "components": [
                                                        {
                                                            "name": "LoadingIndicator"
                                                        }
                                                    ],
                                                    "chunk": "example/pages.json"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
