  "scripts": {
    "build-gulp": "node scripts/build-gulp.js",
    "build-manifest": "npm run build-gulp && gulp",
    "start": "npm run build-manifest && node scripts/start.js",
    "build": "npm run build-manifest && node --max_old_space_size=800 scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "npm run jslint && npm run stylelint",
    "jslint": "eslint src/ --ext .js",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {}
}
